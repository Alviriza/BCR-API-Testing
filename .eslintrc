{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-undef": "off",
        "no-useless-catch": "warn",
        "no-extra-boolean-cast": "off",
        "no-extra-semi": "off"
    }
}
